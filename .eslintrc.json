{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "object-curly-newline": ["error", { "multiline": true }],
    "no-console": "off",
    "comma-dangle": "off",
    "quotes": ["error", "double", { "avoidEscape": true }]
  }
}
